{
  "name": "Hex7",
  "version": "0.0.0",
  "authors": [
    "Laurent <spywen@hotmail.fr>"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "1.3.11",
    "angular-route": "1.3.13",
    "bootstrap-css-only": "3.3.2",
    "angular-bootstrap": "0.12.0",
    "lodash": "3.2.0",
    "angular-hotkeys": "chieffancypants/angular-hotkeys#1.4.5"
  },
  "resolutions": {
    "angular": "1.3.11"
  },
  "overrides": {
    "angular-hotkeys": {
      "main": {
        "dev": [
          "build/hotkeys.js",
          "build/hotkeys.css"
        ],
        "prod": [
          "build/hotkeys.min.js",
          "build/hotkeys.min.css"
        ]
      }
    },
    "angular": {
      "main": {
        "dev": "angular.js",
        "prod": [
          "angular.min.js",
          "angular.min.js.map"
        ]
      }
    },
    "angular-route": {
      "main": {
        "dev": "angular-route.js",
        "prod": [
          "angular-route.min.js",
          "angular-route.min.js.map"
        ]
      }
    },
    "bootstrap-css-only": {
      "main": {
        "dev": [
          "css/bootstrap.css",
          "css/bootstrap.css.map",
          "fonts/glyphicons-halflings-regular.eot",
          "fonts/glyphicons-halflings-regular.svg",
          "fonts/glyphicons-halflings-regular.ttf",
          "fonts/glyphicons-halflings-regular.woff"
        ],
        "prod": [
          "css/bootstrap.min.css",
          "fonts/glyphicons-halflings-regular.eot",
          "fonts/glyphicons-halflings-regular.svg",
          "fonts/glyphicons-halflings-regular.ttf",
          "fonts/glyphicons-halflings-regular.woff"
        ]
      }
    },
    "angular-bootstrap": {
      "main": {
        "dev": "ui-bootstrap.js",
        "prod": "ui-bootstrap.min.js"
      }
    },
    "lodash": {
      "main": {
        "dev": "lodash.js",
        "prod": "lodash.min.js"
      }
    }
  }
}
